#! /bin/bash -x

# =========================================================================================================
printenv 

# =========================================================================================================
/usr/bin/time --verbose -o metrics.out /bin/bash -c "( python -m hstdputils.process $* ) |& tee process.out"

# =========================================================================================================
echo ---------------------------------------- processing log ----------------------------------------------
cat process.out
echo ---------------------------------------- program metrics ---------------------------------------------
cat metrics.out

# =========================================================================================================
export bucket=$1
export prefix=$2
aws s3 cp  --quiet process.out  ${bucket}/${prefix}/
aws s3 cp  --quiet metrics.out  ${bucket}/${prefix}/


